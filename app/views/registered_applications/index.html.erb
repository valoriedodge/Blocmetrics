<h1>My Registered Applications</h1>

<div class="row">
  <div class="col-md-8">
    <% @registered_applications.each do |application| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <% if application.user == current_user %>
            <%= link_to application.name, application.url %>
            <small>
              <%= link_to "Manage Application", registered_application_path(application) %>
            </small>
           <% end %>
          </h4>

        </div>
      </div>
    <% end %>
    <%= link_to "New Application", new_registered_application_path, class: 'btn btn-success' %>
  </div>
</div>
